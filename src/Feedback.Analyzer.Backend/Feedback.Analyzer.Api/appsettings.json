{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  
  "DataAccessSettings": {
    "UseInMemoryDatabase": false
  },
  
  "PasswordValidationSettings": {
    "RequireDigit": true,
    "RequireUppercase": true,
    "RequireLowercase": true,
    "RequireNonAlphanumeric": false,
    "MinimumLength": 8,
    "MaximumLength": 10
  }
}
